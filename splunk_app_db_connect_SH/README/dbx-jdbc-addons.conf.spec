# Contains the list of JDBC Add-ons published in Splunkbase and the connection types supported

[<name>]

name.display = <string>
# Required
# Name to display in the UI

id = <string>
# Required
# Splunkbase app's id

version = <string>
# Required
# Release version in Splunkbase

url = <string>
# Required
# URL to the app in Splunkbase

connection.types = <string>
# Required
# Connection types supported by the driver
# Add multiples types using comma as separator
